                "data.template_file.bastion_user_data": {
                    "type": "template_file",
                    "depends_on": [
                        "aws_network_interface.bastion"
                    ],
                    "primary": {
                        "id": "0df1f0d504d5ba68614ceadc242d1ab44389ef592c65c62bcaed1990b9bb5b29",
                        "attributes": {
