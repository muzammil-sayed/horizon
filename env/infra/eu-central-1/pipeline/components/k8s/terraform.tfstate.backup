                "template_file.cloud_config": {
                    "type": "template_file",
                    "primary": {
                        "id": "d639a044a1e61d487e5b557814b9b5b2aa765e543432ab6b9db2db901c0bc4a3",
                        "attributes": {
                            "id": "d639a044a1e61d487e5b557814b9b5b2aa765e543432ab6b9db2db901c0bc4a3",
                            "rendered": "#cloud-config\ncoreos:\n  units:\n    - name: bootstrap.service\n      command: start\n      content: |\n        [Unit]\n        Requires=early-docker.service\n        After=early-docker.service\n        Description=Bootstraps the node\n        [Service]\n        Environment=\"DOCKER_HOST=unix:///var/run/early-docker.sock\"\n        Type=oneshot\n        RemainAfterExit=yes\n        ExecStart=/root/bootstrap.sh\n    - name: settimezone.service\n      command: start\n      content: |\n        [Unit]\n        Description=Set the timezone\n        [Service]\n        ExecStart=/usr/bin/timedatectl set-timezone UTC\n        RemainAfterExit=yes\n        Type=oneshot\nupdate:\n    reboot-strategy: off\nssh_authorized_keys:\n  - ssh-rsa \nwrite_files:\n  - path: /root/bootstrap.sh\n    permissions: 0755\n    content: |\n      #!/usr/bin/bash\n      #\n      if [ -f /etc/coreos/update.conf ] ; then\n        rm  /etc/coreos/update.conf\n      fi\n\n      ARTIFACT_BOOTSTRAP_PREFIX=ansible-coreos-bootstrap\n      ARTIFACT_PREFIX=ansible-coreos\n      SERVER=10.160.70.25\n      ARTIFACT_PATH=orphans/ansible-coreos\n\n      # $KubernetesCluster is passed from terraform\n      ansible_build_artifact=\"${ARTIFACT_PREFIX}-eu-central-1_jive-infra-pipeline\"\n\n      curl -s -o /var/tmp/${ARTIFACT_PREFIX}.tar.gz  http://${SERVER}/${ARTIFACT_PATH}/${ansible_build_artifact}.tar.gz\n      sudo tar -xzf /var/tmp/${ARTIFACT_PREFIX}.tar.gz -C /srv\n\n      chmod +x /srv/ansible-coreos/inventory_ec2/ec2.py\n      bash -vx /srv/ansible-coreos/bin/run-ansible-container.sh\n\n      exit 0\n  - path: /etc/ntp.conf\n    content: |\n      driftfile /var/lib/ntp/ntp.drift\n      restrict default kod nomodify notrap nopeer noquery\n      restrict 127.0.0.1\n      restrict -6 ::1\n      server 0.amazon.pool.ntp.org iburst minpoll 4 maxpoll 5\n      server 1.amazon.pool.ntp.org iburst minpoll 4 maxpoll 5\n      server 2.amazon.pool.ntp.org iburst minpoll 4 maxpoll 5\n      server 3.amazon.pool.ntp.org iburst minpoll 4 maxpoll 5\n      includefile /etc/ntp/crypto/pw\n      keys /etc/ntp/keys\n      restrict -6 default kod nomodify notrap nopeer noquery\nwrite_files:\n  - path: /etc/hosts\n    permissions: 0644\n    content: |\n      10.160.4.127 docker.phx1.jivehosted.com\n      10.160.70.25 pulp-slave\n",
